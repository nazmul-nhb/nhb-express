    "check:d": "tsc -p ./templates/drizzle/tsconfig.json --noEmit",
    "check:p": "tsc -p ./templates/prisma/tsconfig.json --noEmit",
    "check": "pnpm check:m && pnpm check:d && pnpm check:p",
    "prepare": "husky"
  },
  "publishConfig": {
